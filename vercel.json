{
    "version": 2,
    "builds": [
    { "src": "client/index.html", "use": "@vercel/static-build", "config": { "distDir": "client/dist" } },
    { "src": "server/src/index.js", "use": "@vercel/node" }
    ],
    "routes": [
    { "src": "/api/(.*)", "dest": "server/src/index.js" },
    { "src": "/(.*)", "dest": "client/dist/$1" }
    ]
    }